    # pha "I'm not worried about myself Professor..! I'm worried about you..!"
    voice "audio/phanta_voice/lvl3/pha25.wav"
    pha "Je ne m'inquiète pas pour moi Professeur..! Je m'inquiète pour toi..!"

# game/routes/phanta.rpy:596
translate French phanta_lvl3_e5bd5c34:

    # voice "audio/phanta_voice/lvl3/pha26.wav"
    # pha "Please, I want to keep seeing you... even if my dad doesn't approve..!"
    voice "audio/phanta_voice/lvl3/pha26.wav"
    pha "S'il vous plaît, je veux continuer à vous voir... même si mon père n'approuve pas... !"

# game/routes/phanta.rpy:598
translate French phanta_lvl3_12593a0d:

